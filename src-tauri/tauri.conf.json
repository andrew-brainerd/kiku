{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "kiku",
  "version": "0.1.0",
  "identifier": "com.kikuapp.main",
  "build": {
    "beforeDevCommand": "pnpm run dev:vite",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "kiku",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all"
  }
}
